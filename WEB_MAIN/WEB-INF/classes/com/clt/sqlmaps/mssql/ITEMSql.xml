<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="ITEM">
<typeAlias alias="Grd12IMGroupVO" 	type="com.clt.apps.fis.wms.item.dto.Grd12IMGroupVO"/>
<typeAlias alias="Grd11IMGroupVO" 	type="com.clt.apps.fis.wms.item.dto.Grd11IMGroupVO"/>
<select id="searchItemGroupCodeList" resultClass="Grd11IMGroupVO">
	<!-- /*searchItemGroupList*/ -->
		SELECT  MAX(IG.ITEM_GRP_CD) 			AS ITEM_GRP_CD
			   ,MAX(IG.ITEM_GRP_NM)				AS ITEM_GRP_NM
			   ,MAX(IG.CTRT_NO)					AS CTRT_NO
			   ,MAX(dbo.TL_GET_CTRT_NM(IG.CTRT_NO)) AS CTRT_NM
				,
			 (
				(
					SELECT
				COUNT (*)
			FROM
				TL_CTRT_CUST_ITEM A, TL_CTRT_CUST_ITEM_GRP B
			WHERE
				A.CTRT_NO = B.CTRT_NO
			AND A.ITEM_GRP_CD = B.ITEM_GRP_CD
				)
			) AS ITEM_CNT
						   ,MAX(IG.USE_FLG) 				AS USE_FLG
					  FROM TL_CTRT_CUST_ITEM_GRP IG LEFT JOIN TL_CTRT_CUST_ITEM IT ON IG.CTRT_NO     = IT.CTRT_NO
								 AND IG.ITEM_GRP_CD = IT.ITEM_GRP_CD
		      
		 WHERE 1=1
		
	 <isNotEmpty property="in_ctrt_no">
		   AND IG.CTRT_NO = #in_ctrt_no#
		 </isNotEmpty>
		 <isNotEmpty property="in_grp_cd">
		   AND IG.ITEM_GRP_CD = #in_grp_cd#
		 </isNotEmpty>
		 <isNotEmpty property="in_grp_nm">
		   AND IG.ITEM_GRP_NM LIKE '%' + #in_grp_nm# + '%'
		 </isNotEmpty> 
		 <isNotEmpty property="in_item_cd">
		   AND IT.ITEM_CD = #in_item_cd#
		 </isNotEmpty>
		 <isNotEmpty property="in_item_nm">
		   AND IT.ITEM_NM LIKE '%' + #in_item_nm# + '%'
		 </isNotEmpty> 
		 <isNotEmpty property="in_use_sel">
		   AND ISNULL(IG.USE_FLG, 'N') = #in_use_sel#
		 </isNotEmpty>
		 GROUP BY IG.ITEM_GRP_CD
		 ORDER BY IG.ITEM_GRP_CD
	</select>
	
	<select id="searchItemGroupCodeListAll" resultClass="Grd11IMGroupVO">
	<!-- /*searchItemGroupList*/ -->
		SELECT A.* from (SELECT
			IG.ITEM_GRP_CD AS ITEM_GRP_CD,
			IG.ITEM_GRP_NM AS ITEM_GRP_NM,
			IG.CTRT_NO AS CTRT_NO,
			dbo.TL_GET_CTRT_NM (IG.CTRT_NO) AS CTRT_NM,
		(
			(
			SELECT
				COUNT (*)
			FROM
				TL_CTRT_CUST_ITEM A,
				TL_CTRT_CUST_ITEM_GRP B
			WHERE
				A.CTRT_NO = B.CTRT_NO
			AND A.ITEM_GRP_CD = B.ITEM_GRP_CD
			)
		) AS ITEM_CNT,
 			IG.USE_FLG AS USE_FLG
		FROM
		TL_CTRT_CUST_ITEM_GRP IG
		LEFT JOIN TL_CTRT_CUST_ITEM IT ON IG.CTRT_NO = IT.CTRT_NO
		AND IG.ITEM_GRP_CD = IT.ITEM_GRP_CD
		) A
	WHERE item_grp_cd = 'KIT'
	</select>
	
	<select id="searchSubItemGroupCodeList" resultClass="Grd12IMGroupVO">
		SELECT IG.ITEM_GRP_CD
		      ,IT.ITEM_CD
		      ,IT.ITEM_NM
		  FROM TL_CTRT_CUST_ITEM_GRP IG
		      ,TL_CTRT_CUST_ITEM IT
		 WHERE 1=1
		   AND IG.CTRT_NO     = IT.CTRT_NO
		   AND IG.ITEM_GRP_CD = IT.ITEM_GRP_CD
		   AND IG.ITEM_GRP_CD = #in_grp_cd# 
		   AND IG.CTRT_NO 	  = #in_ctrt_no#
		 ORDER BY IT.ITEM_CD
	</select>
	
	<insert id="addItemGroupList">
      INSERT INTO TL_CTRT_CUST_ITEM_GRP (
		          CTRT_NO,
			      ITEM_GRP_CD,
			      ITEM_GRP_NM,
			      USE_FLG,
			      RGST_ID,
			      RGST_SYS_DT,
			      MODI_ID,
			      MODI_SYS_DT
      )VALUES(
      			 #ctrt_no#
                ,#item_grp_cd#
                ,#item_grp_nm#
                ,#use_flg#
                ,#user_id#
                ,GETDATE()
                ,#user_id#
                ,GETDATE()
      )
    </insert>
    
    <update id="modifyItemGroupList">
    	UPDATE TL_CTRT_CUST_ITEM_GRP
    	   SET ITEM_GRP_NM = #item_grp_nm#
    	      ,USE_FLG	   = #use_flg#
    	 WHERE CTRT_NO 		= #ctrt_no#
    	   AND ITEM_GRP_CD 	= #item_grp_cd#
    </update>
    
    <update id="modifyItemGroupCode">
    	UPDATE TL_CTRT_CUST_ITEM
    	   SET ITEM_GRP_CD = ISNULL
    	 WHERE CTRT_NO 		= #ctrt_no#
    	   AND ITEM_GRP_CD 	= #item_grp_cd# 
    </update>
</sqlMap>
<!-- <?xml version="1.0" encoding="UTF-8" ?> 
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" "http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">

<mapper namespace="ITEM">
	
	<resultMap type="com.clt.business.oms.item.dto.ITListDto" id="ITListDto">
	</resultMap>
	
	<select id="searchItemList" resultMap="ITListDto">
	/*searchItemList*/
	SELECT	 ROW_NUMBER() OVER(ORDER BY ITEM_CD) AS SEQ
		    ,ITEM_CD         
	        ,ITEM_NM      
	        ,ITEM_GRP_CD   
		    ,CTRT_NO         
	        ,TL_GET_CTRT_NM(CTRT_NO) AS CTRT_NM         
	        ,HTS_NO          
			,TRIM(TO_CHAR(LV1_CBM, '9,999,999,990.99999'))      AS ITEM_CBM            
			,TRIM(TO_CHAR(LV1_GRS_KGS, '9,999,999,990.999'))    AS ITEM_KGS  
			,TRIM(TO_CHAR(LV1_NET_KGS, '9,999,999,990.999'))  	AS ITEM_NET_WGT  
			,TRIM(TO_CHAR(ITEM_WIDTH, '9,999,999,990.99'))      AS ITEM_WIDTH  
			,TRIM(TO_CHAR(ITEM_LENGTH, '9,999,999,990.99'))     AS ITEM_LENGTH  
			,TRIM(TO_CHAR(ITEM_HEIGHT, '9,999,999,990.99'))     AS ITEM_HEIGHT 
			,PKG_LV1_UNIT_CD
			,TRIM(TO_CHAR(PKG_LV1_QTY, '9,999,999,990'))    	AS PKG_LV1_QTY
			,ITEM_PKGUNIT
			,TRIM(TO_CHAR(ITEM_PKGBASEQTY, '9,999,999,990')) 	AS ITEM_PKGBASEQTY
			,PKG_LV3_UNIT_CD
			,TRIM(TO_CHAR(PKG_LV3_QTY, '9,999,999,990'))    	AS PKG_LV3_QTY
			,PKG_LV4_UNIT_CD
			,TRIM(TO_CHAR(PKG_LV4_QTY, '9,999,999,990'))    	AS PKG_LV4_QTY
			,ITEM_USE_FLG    
			,RGST_ID
			,TO_CHAR(RGST_SYS_DT, 'YYYY-MM-DD')					AS RGST_SYS_DT
			,MODI_ID
			,TO_CHAR(MODI_SYS_DT, 'YYYY-MM-DD')					AS MODI_SYS_DT 									
			,ITEM_REMARK     
			,CTRT_NO
			,ITEM_SYS_NO
			,COUNT(*) OVER() AS TOTAL
	  FROM(
			SELECT 
				    I.CTRT_NO
			       ,I.ITEM_SYS_NO
			       ,I.ITEM_CD 
			       ,I.ITEM_NM
			       ,I.ITEM_GRP_CD
			       ,I.HTS_NO
			       ,TL_GET_HTS_NM(I.HTS_NO) AS HTS_NM   
			       ,I.ITEM_CBM 
			       ,I.ITEM_KGS 
			       ,I.ITEM_WIDTH 
			       ,I.ITEM_LENGTH
			       ,I.ITEM_HEIGHT
			       ,I.ITEM_REMARK
			       ,I.ITEM_USE_FLG
			       ,I.ITEM_NET_WGT
			       ,I.LV1_CBM
			       ,I.LV1_GRS_KGS
			       ,I.LV1_NET_KGS
			       ,I.PKG_LV1_UNIT_CD
				   ,I.PKG_LV1_QTY
				   ,I.ITEM_PKGUNIT
				   ,I.ITEM_PKGBASEQTY
				   ,I.PKG_LV3_UNIT_CD
				   ,I.PKG_LV3_QTY
				   ,I.PKG_LV4_UNIT_CD
				   ,I.PKG_LV4_QTY
				   ,I.RGST_ID
				   ,I.RGST_SYS_DT
				   ,I.MODI_ID
				   ,I.MODI_SYS_DT
			  FROM TL_CTRT_CUST_ITEM I
			 WHERE 1=1
			   AND I.ITEM_CD IS NOT NULL
			 <if test="ctrt_no != '' ">
			 	AND I.CTRT_NO = #ctrt_no, javaType=String, jdbcType=VARCHAR#
			 </if>  
	         <if test="cust_item_no != '' ">
	         	AND I.ITEM_CD = #cust_item_no, javaType=String, jdbcType=VARCHAR#
	         </if>
	         <if test="cust_item_nm != '' ">
	         	AND I.ITEM_NM LIKE '%' || #cust_item_nm, javaType=String, jdbcType=VARCHAR# || '%'
	         </if>
			 <if test="use_sel != '' ">
			 	AND I.ITEM_USE_FLG = #use_sel, javaType=String, jdbcType=VARCHAR#
			 </if>
			 <if test="grp_cd != '' ">
			 	AND I.ITEM_GRP_CD = #grp_cd, javaType=String, jdbcType=VARCHAR#
			 </if>
			 <if test="supp_cd != '' ">
				 AND EXISTS (SELECT NULL
				               FROM TL_CTRT_CUST_ITEM_SUPP S
				              WHERE S.CTRT_NO = I.CTRT_NO
				                AND S.ITEM_SYS_NO = I.ITEM_SYS_NO
				                AND S.SUPP_CD = #supp_cd, javaType=String, jdbcType=VARCHAR# )
			  </if>
		   )         
	</select>
	
	
	
	<resultMap type="com.clt.business.oms.item.dto.IMDto" id="IMDto">
	</resultMap>
	
	<select id="searchItemExists" resultMap="IMDto">
	/*searchItemExists*/
		SELECT  NVL(MAX(CTRT_NO), #ctrt_no, javaType=String, jdbcType=VARCHAR# )        AS CTRT_NO
		       ,NVL(MAX(ITEM_CD), #item_cd, javaType=String, jdbcType=VARCHAR# )        AS ITEM_CD
		       ,NVL(MAX(ITEM_NM), '')              AS ITEM_NM
		       ,NVL(MAX(ITEM_SYS_NO), '')          AS ITEM_SYS_NO
		       ,NVL(COUNT(ITEM_SYS_NO), '0')       AS ROW_CNT
		  FROM TL_CTRT_CUST_ITEM
		 WHERE 1=1
		   AND CTRT_NO = #ctrt_no, javaType=String, jdbcType=VARCHAR# 
		   AND ITEM_CD = #item_cd, javaType=String, jdbcType=VARCHAR# 
		   AND ITEM_USE_FLG = 'Y'      
	</select>
	
	
	
	
	ITEM SYS NO 채번
	<select id="searchItemSysNo" resultType="String">
	/*searchItemSysNo*/
		SELECT 'IT' 
		       ||(SELECT CTRT_CUST_CD FROM TL_CTRT WHERE CTRT_NO = #ctrt_no, javaType=String, jdbcType=VARCHAR# ) 
		       || TO_CHAR(SYSDATE,'YYMMDD') || LPAD(ITEM_SEQ.NEXTVAL,5,'0') AS ITEM_SYS_NO
		  FROM DUAL     
	</select>
	
	
	<resultMap type="com.clt.business.oms.item.dto.IMInfoDto" id="IMInfoDto">
	</resultMap>
	
	<select id="searchIMInfo" resultMap="IMInfoDto">
	/*searchIMInfo*/
		SELECT  I.CTRT_NO, 
		        TL_GET_CTRT_NM(I.CTRT_NO) AS CTRT_NM,
		        I.ITEM_SYS_NO, 
		        I.ITEM_CD, 
		        I.ITEM_NM, 
		        I.HTS_NO, 
		        TL_GET_HTS_NM(I.HTS_NO) AS HTS_NM,     
		            
		        
		        TRIM(TO_CHAR(I.LV1_CBM, '9,999,999,990.99999'))     AS LV1_CBM, 
                TRIM(TO_CHAR(I.LV1_CBF, '9,999,999,990.99999'))     AS LV1_CBF,  
		        TRIM(TO_CHAR(I.LV1_GRS_KGS, '9,999,999,990.999'))   AS LV1_GRS_KGS, 
		        TRIM(TO_CHAR(I.LV1_GRS_LBS, '9,999,999,990.999'))   AS LV1_GRS_LBS, 
		        TRIM(TO_CHAR(I.LV1_NET_KGS, '9,999,999,990.999'))   AS LV1_NET_KGS,
		        TRIM(TO_CHAR(I.LV1_NET_LBS, '9,999,999,990.999'))   AS LV1_NET_LBS,
		        TRIM(TO_CHAR(I.LV1_WIDTH, '9,999,999,990.99'))      AS LV1_WIDTH, 
		        TRIM(TO_CHAR(I.LV1_LENGTH, '9,999,999,990.99'))     AS LV1_LENGTH, 
		        TRIM(TO_CHAR(I.LV1_HEIGHT, '9,999,999,990.99'))     AS LV1_HEIGHT, 
		         
		        TRIM(TO_CHAR(I.ITEM_CBM, '9,999,999,990.99999'))    AS ITEM_CBM, 
                TRIM(TO_CHAR(I.ITEM_CBF, '9,999,999,990.99999'))    AS ITEM_CBF,  
		        TRIM(TO_CHAR(I.ITEM_KGS, '9,999,999,990.999'))      AS ITEM_KGS, 
		        TRIM(TO_CHAR(I.item_grs_lbs, '9,999,999,990.999'))  AS item_grs_lbs, 
		        TRIM(TO_CHAR(I.ITEM_NET_WGT, '9,999,999,990.999'))  AS ITEM_NET_WGT,
		        TRIM(TO_CHAR(I.item_net_lbs, '9,999,999,990.999'))  AS item_net_lbs,
		        TRIM(TO_CHAR(I.ITEM_WIDTH, '9,999,999,990.99'))     AS ITEM_WIDTH, 
		        TRIM(TO_CHAR(I.ITEM_LENGTH, '9,999,999,990.99'))    AS ITEM_LENGTH, 
		        TRIM(TO_CHAR(I.ITEM_HEIGHT, '9,999,999,990.99'))    AS ITEM_HEIGHT, 
		        
		        TRIM(TO_CHAR(I.LV3_CBM, '9,999,999,990.99999'))     AS LV3_CBM, 
                TRIM(TO_CHAR(I.LV3_CBF, '9,999,999,990.99999'))     AS LV3_CBF, 
                TRIM(TO_CHAR(I.LV3_GRS_KGS, '9,999,999,990.999'))   AS LV3_GRS_KGS, 
                TRIM(TO_CHAR(I.LV3_GRS_LBS, '9,999,999,990.999'))   AS LV3_GRS_LBS, 
                TRIM(TO_CHAR(I.LV3_NET_KGS, '9,999,999,990.999'))   AS LV3_NET_KGS,
                TRIM(TO_CHAR(I.LV3_NET_LBS, '9,999,999,990.999'))   AS LV3_NET_LBS,
                TRIM(TO_CHAR(I.LV3_WIDTH, '9,999,999,990.99'))      AS LV3_WIDTH, 
                TRIM(TO_CHAR(I.LV3_LENGTH, '9,999,999,990.99'))     AS LV3_LENGTH, 
                TRIM(TO_CHAR(I.LV3_HEIGHT, '9,999,999,990.99'))     AS LV3_HEIGHT, 
                
                NVL(TRIM(TO_CHAR(I.PKG_PL_STD_QTY, '9,999,999,990')), TRIM(TO_CHAR(I.PKG_LV3_QTY, '9,999,999,990')) )   AS PKG_PL_STD_QTY, 
                NVL(TRIM(TO_CHAR(I.PKG_PL_OVER_WGT, '9,999,999,990.9')), '1.0')                                         AS PKG_PL_OVER_WGT, 
		        
		        I.ITEM_REMARK, 
		        I.ITEM_USE_FLG,
		        I.ITEM_GRP_CD,
		        
		        I.PKG_LV1_UNIT_CD,
		        TL_GET_COMM_NM('A6',I.PKG_LV1_UNIT_CD) 				AS PKG_LV1_UNIT_NM,
		        TRIM(TO_CHAR(I.PKG_LV1_QTY, '9,999,999,990'))		AS PKG_LV1_QTY,
		        NVL(I.PKG_LV1_PUT_TP_CD, 'N')						AS PKG_LV1_PUT_TP_CD,
		  		
		  		I.ITEM_PKGUNIT,
		  		TL_GET_COMM_NM('A6',I.ITEM_PKGUNIT) 				AS ITEM_PKGUNIT_NM,
		        TRIM(TO_CHAR(I.ITEM_PKGBASEQTY, '9,999,999,990')) 	AS ITEM_PKGBASEQTY, 
		        NVL(I.PKG_LV2_PUT_TP_CD, 'N')						AS PKG_LV2_PUT_TP_CD,
		        
		  		I.PKG_LV3_UNIT_CD,
		  		TL_GET_COMM_NM('A6',I.PKG_LV3_UNIT_CD) 				AS PKG_LV3_UNIT_NM,
		  		TRIM(TO_CHAR(I.PKG_LV3_QTY, '9,999,999,990'))		AS PKG_LV3_QTY,
		  		NVL(I.PKG_LV3_PUT_TP_CD, 'N')						AS PKG_LV3_PUT_TP_CD,
		  		
		  		I.PKG_LV4_UNIT_CD,
		  		TL_GET_COMM_NM('A6',I.PKG_LV4_UNIT_CD) 				AS PKG_LV4_UNIT_NM,
		  		TRIM(TO_CHAR(I.PKG_LV4_QTY, '9,999,999,990'))		AS PKG_LV4_QTY,
		  		NVL(I.PKG_LV4_PUT_TP_CD, 'N')						AS PKG_LV4_PUT_TP_CD,
		  		
		  		I.ALTER_ITEM_CD,
		  		I.BARCODE_NO,
		  		TRIM(TO_CHAR(I.SAFE_STC_QTY, '9,999,999,990'))		AS SAFE_STC_QTY,
		  		I.ADV_CURR_CD,
		  		TRIM(TO_CHAR(I.ADV_PRICE, '9,999,999,990.99'))		AS ADV_PRICE,
		  		I.NEGO_CURR_CD,
		  		TRIM(TO_CHAR(I.NEGO_PRICE, '9,999,999,990.99'))		AS NEGO_PRICE,
		  		I.UNIT_CURR_CD,
		  		TRIM(TO_CHAR(I.UNIT_PRICE, '9,999,999,990.99'))		AS UNIT_PRICE,
		  		I.ABC_CD,
		  		I.REF_CD_01,
		  		I.REF_CD_02
		  		 
		  FROM TL_CTRT_CUST_ITEM I 
		 WHERE I.CTRT_NO 		= #in_ctrt_no,  javaType=String, jdbcType=VARCHAR#
		 <if test="item_sys_no != '' ">
		 	AND I.ITEM_SYS_NO	= #item_sys_no, javaType=String, jdbcType=VARCHAR#
		 </if>
		 <if test="item_sys_no == '' ">
		 	AND I.ITEM_USE_FLG = 'Y'
		 </if>
		 <if test="in_item_cd != '' ">
		 	AND I.ITEM_CD	 	= #in_item_cd,  javaType=String, jdbcType=VARCHAR#
		 </if>
		   
	</select>
	
	
	<resultMap type="com.clt.business.oms.item.dto.IMSuppDto" id="IMSuppDto">
	</resultMap>
	
	<select id="searchIMSuppList" resultMap="IMSuppDto">
	/*searchIMSuppList*/
		SELECT ROW_NUMBER() OVER(ORDER BY I.ITEM_SYS_NO) AS SEQ,
		       I.CTRT_NO, 
		       I.ITEM_SYS_NO, 
		       I.SUPP_CD, 
		       TL_GET_CUST_NM(I.SUPP_CD) AS SUPP_NM,
		       I.SUPP_ITEM_CD,
		       I.CTRT_NO,
		       I.ITEM_SYS_NO,
		       COUNT(*) OVER() AS TOTAL
		  FROM TL_CTRT_CUST_ITEM_SUPP I 
		 WHERE I.CTRT_NO 		= #ctrt_no, 	 javaType=String, jdbcType=VARCHAR#
		   AND I.ITEM_SYS_NO	= #item_sys_no, javaType=String, jdbcType=VARCHAR#
	</select>
	
	<resultMap type="com.clt.business.oms.item.dto.IMWhDto" id="IMWhDto">
	</resultMap>
	
	<select id="searchIMWarehouseList" resultMap="IMWhDto">
	/*searchIMWarehouseList*/
		SELECT ROW_NUMBER() OVER(ORDER BY I.ITEM_SYS_NO) AS SEQ,
		       I.CTRT_NO, 
		       I.ITEM_SYS_NO, 
		       I.WH_CD, 
		       TL_GET_LOC_NM(I.wh_cd) AS WH_NM,
		       I.FIX_LOC_CD,
		       (SELECT WH_LOC_NM FROM TL_WH_LOC WHERE LOC_CD = I.WH_CD AND WH_LOC_CD = I.FIX_LOC_CD) AS FIX_LOC_NM,
		       I.DEF_LOC_CD,
		       (SELECT WH_LOC_NM FROM TL_WH_LOC WHERE LOC_CD = I.WH_CD AND WH_LOC_CD = I.DEF_LOC_CD) AS DEF_LOC_NM,
		       COUNT(*) OVER() AS TOTAL
		  FROM TL_CTRT_CUST_ITEM_WH I 
		 WHERE I.CTRT_NO 		= #ctrt_no, 	 javaType=String, jdbcType=VARCHAR#
		   AND I.ITEM_SYS_NO	= #item_sys_no, javaType=String, jdbcType=VARCHAR#
	</select>
	
	<resultMap type="com.clt.business.oms.item.dto.IMFileDto" id="IMFileDto">
	</resultMap>
	
	<select id="searchIMFileList" resultMap="IMFileDto">
	/*searchIMFileList*/
		SELECT ROW_NUMBER() OVER(ORDER BY I.ITEM_SYS_NO) AS SEQ,
		       I.CTRT_NO, 
		       I.ITEM_SYS_NO, 
		       I.file_seq, 
		       I.file_path,
		       I.file_sys_nm,
		       I.file_org_nm,
		       I.file_size,
		       COUNT(*) OVER() AS TOTAL
		  FROM TL_CTRT_CUST_ITEM_FILE_REP I 
		 WHERE I.CTRT_NO 		= #ctrt_no, 	 javaType=String, jdbcType=VARCHAR#
		   AND I.ITEM_SYS_NO	= #item_sys_no, javaType=String, jdbcType=VARCHAR#
	</select>
	
	
	ITEM INFO 저장
	<insert id="addItemInfo">
		INSERT INTO TL_CTRT_CUST_ITEM(
				CTRT_NO,
		        ITEM_SYS_NO,
		        ITEM_CD,
		        ITEM_NM,
		        HTS_NO,
		        
		        LV1_CBM,
		        LV1_CBF,
		        LV1_GRS_KGS,
		        LV1_GRS_LBS,
		        LV1_NET_KGS,
		        LV1_NET_LBS,
		        LV1_WIDTH,
		        LV1_LENGTH,
		        LV1_HEIGHT,
		        
		        ITEM_CBM,
		        ITEM_CBF,
		        ITEM_KGS,
		        ITEM_GRS_LBS,
		        ITEM_NET_WGT,
		        ITEM_NET_LBS,
		        ITEM_WIDTH,
		        ITEM_LENGTH,
		        ITEM_HEIGHT,
		        
		        ITEM_REMARK,
		        ITEM_USE_FLG,
		        RGST_ID,
		        RGST_SYS_DT,
		  		ITEM_GRP_CD,
		  		
		  		PKG_LV1_UNIT_CD,
		  		PKG_LV1_QTY,
		  		PKG_LV1_PUT_TP_CD,
		        ITEM_PKGUNIT,
		        ITEM_PKGBASEQTY,
		  		PKG_LV2_PUT_TP_CD,
		  		PKG_LV3_UNIT_CD,
		  		PKG_LV3_QTY,
		  		PKG_LV3_PUT_TP_CD,
		  		PKG_LV4_UNIT_CD,
		  		PKG_LV4_QTY,
		  		PKG_LV4_PUT_TP_CD,
		  		
		  		ALTER_ITEM_CD,
		  		BARCODE_NO,
		  		SAFE_STC_QTY,
		  		ADV_CURR_CD,
		  		ADV_PRICE,
		  		NEGO_CURR_CD,
		  		NEGO_PRICE,
		  		UNIT_CURR_CD,
		  		UNIT_PRICE,
		  		ABC_CD,
		  		REF_CD_01,
		  		REF_CD_02,
		  		
		  		LV3_CBM,
                LV3_CBF,
                LV3_GRS_KGS,
                LV3_GRS_LBS,
                LV3_NET_KGS,
                LV3_NET_LBS,
                LV3_WIDTH,
                LV3_LENGTH,
                LV3_HEIGHT,
                
                PKG_PL_STD_QTY,
                PKG_PL_OVER_WGT
		  		 
	    )VALUES (
		  		#ctrt_no, 			javaType=String, jdbcType=VARCHAR#,
